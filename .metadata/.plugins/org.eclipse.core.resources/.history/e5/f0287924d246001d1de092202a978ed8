package collection;

import java.util.ArrayList;

public class ArraylistUse
{
	public static void main(String[] args)
	{
		ArrayList<Object> al = new ArrayList<>();
		
		al.add("Uma");
		al.add("Sathiya");
		al.add("Advik");
		al.add(null);
		al.add(null);
		al.add(true);
		al.add(2145);
		al.add(null);
		al.add("Mahesh");
		al.add("Seelan");
		
		System.out.println(al);
		
		al.add(3, 12.5f);
		System.out.println(al);

//		al.clear();
//		System.out.println(al);
		
		Object Al = al.clone();
		System.out.println(Al);
		
		System.out.println(al.equals(Al));
		
		System.out.println(al.get(2));
	}

}
