package string;

public class Stringmethods
{

	public static void main(String[] args) 
	{
		//contains() method
		String a="Wagholi";
		System.out.println(a.contains("ag"));
		System.out.println(a.contains("holi"));
		
		//isEmpty() method
		
		System.out.println(a.isEmpty());
		String a1="";
		String a2="  ";
		System.out.println(a1.isEmpty());
		System.out.println(a2.isEmpty());
		
		//isBlank() method
		System.out.println(a.isBlank());
		System.out.println(a1.isBlank());
		System.out.println(a2.isBlank());
		
		//charAt() method
		
		String b="Mock Group 13";
		System.out.println(b.charAt(3));
		//System.out.println(b.charAt(16));	//Exceeds the index limit
		
		//endsWith() method
		
		String c="Pune";
		System.out.println(c.endsWith("e"));
		System.out.println(c.endsWith("ne"));
		
		//StartsWith() method
		
		String d="Refridgerator";
		System.out.println(d.startsWith("R"));
		System.out.println(d.startsWith("rid", 3));
		
		//concat() method
		System.out.println(a.concat(c));
		
		//substring() method
		System.out.println(d.substring(2));
		System.out.println(b.substring(5, 10));
		
		//replace() method
		String e="choose";
		System.out.println(e.replace("o", "e"));
		
		//index() method
		System.out.println(e.indexOf("o"));
		
		//lastindexof() method
		System.out.println(e.lastIndexOf("o", 2));
		
		
		
		}

}
